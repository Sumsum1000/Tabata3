<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ProgressBar
        android:id="@+id/progress_bar_rounds"
        style="@style/Base.Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:progress="90"
        android:progressDrawable="@drawable/circle_rounds"
        tools:layout_editor_absoluteX="254dp"
        tools:layout_editor_absoluteY="25dp" />

    <ProgressBar
        android:id="@+id/progress_rest"
        style="@style/Base.Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:progress="90"
        android:progressDrawable="@drawable/circle_rest"
        tools:layout_editor_absoluteX="203dp"
        tools:layout_editor_absoluteY="153dp" />

    <ProgressBar
        android:id="@+id/progress_bar_work"
        style="@style/Base.Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:progress="90"
        android:progressDrawable="@drawable/circle_work"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="38dp" />

    <TextView
        android:id="@+id/tv_rounds"
        android:layout_width="48dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Rounds"
        android:textSize="12dp"
        tools:layout_editor_absoluteX="266dp"
        tools:layout_editor_absoluteY="50dp" />


    <TextView
        android:id="@+id/tv_seconds"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="rest"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="223dp"
        tools:layout_editor_absoluteY="188dp" />

    <TextView
        android:id="@+id/tv_work"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Work"
        android:textSize="30dp"
        tools:layout_editor_absoluteX="63dp"
        tools:layout_editor_absoluteY="88dp" />

    <SeekBar
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="168dp"
        tools:layout_editor_absoluteY="382dp" />

    <SeekBar
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="168dp"
        tools:layout_editor_absoluteY="466dp" />

    <SeekBar
        android:id="@+id/seekBar"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="168dp"
        tools:layout_editor_absoluteY="423dp" />

    <TextView
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:text="Rounds"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="457dp" />

    <TextView
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:text="Rest"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="423dp" />

    <TextView
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:text="Work"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="382dp" />


    <TextView
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:text="10:00:00"
        android:textSize="15dp"
        tools:layout_editor_absoluteX="88dp"
        tools:layout_editor_absoluteY="388dp" />

    <TextView
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:text="20"
        android:textSize="15dp"
        android:gravity="center"
        tools:layout_editor_absoluteX="88dp"
        tools:layout_editor_absoluteY="423dp" />

    <TextView
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:text="999"
        android:gravity="center"
        android:textSize="15dp"
        tools:layout_editor_absoluteX="88dp"
        tools:layout_editor_absoluteY="466dp" />


</android.support.constraint.ConstraintLayout>